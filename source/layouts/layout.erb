                                              <%= c[0] %></a> 
                                          <% else %>
                                            <%= c[0] %>
                                          <% end %>

                                          <% if (c[1].is_a?(Hash)) && (c[1].key?("type")) %>
                                            <br> <%= c[1]['type'] %>
                                          <% end %>
                                          
                                        </div>
                                        <div class="div-table-col description" style="background-color: #eaf2f6;">
                                          <% if (c[1].is_a?(Hash)) && (c[1].key?("documentation")) %>
                                            <%= c[1]['documentation']['description'] %> 
                                          <% end %>
                                          
                                        </div>
                                      </div>

                                      <% if (c[1].is_a?(Hash)) %> 

